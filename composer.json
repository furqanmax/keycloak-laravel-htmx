{
    "name": "keycloak-auth/laravel",
    "description": "Laravel integration package for Keycloak authentication with HTMX support",
    "keywords": ["laravel", "keycloak", "authentication", "oauth", "htmx", "sso"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Keycloak Auth SDK",
            "email": "support@keycloak-auth-sdk.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^6.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "KeycloakAuth\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeycloakAuth\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KeycloakAuth\\Laravel\\Providers\\KeycloakAuthServiceProvider"
            ],
            "aliases": {
                "KeycloakAuth": "KeycloakAuth\\Laravel\\Facades\\KeycloakAuth"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
